    </sequenceFlow>
    <sequenceFlow id="line8" name="驳回" sourceRef="judge2" targetRef="leaderApproval">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${back=='leader'&&approval==false}]]></conditionExpression>
