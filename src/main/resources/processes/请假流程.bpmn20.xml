      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${approval==false}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="line9" name="驳回" sourceRef="judge2" targetRef="apply1">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${back!='leader'&&approval==false}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="line3" name="发起人是团队长" sourceRef="roleJudge" targetRef="managerrApproval">
      <documentation>发起人是团队长</documentation>
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${role=='leader'}]]></conditionExpression>
    </sequenceFlow>
